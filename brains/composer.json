{
    "name": "johnny-deployer/brains",
    "description": "Johnny's brains",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer-bundle": "^2.0",
        "symfony/asset": "^3.3",
        "symfony/console": "^3.3",
        "symfony/dotenv": "^3.3",
        "symfony/expression-language": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/framework-bundle": "^3.3",
        "symfony/monolog-bundle": "^3.1",
        "symfony/property-info": "^3.3",
        "symfony/security-bundle": "^3.3",
        "symfony/serializer": "^3.3",
        "symfony/validator": "^3.3",
        "symfony/yaml": "^3.3"
    },
    "authors": [
        {
            "name": "Georgiana Gligor",
            "email": "gb@tekkie.ro"
        }
    ],
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BNJF5FVPFHY0AGSW38NEKRCD",
            "allow-contrib": "true"
        }
    }
}
